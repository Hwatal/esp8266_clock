{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/build/include",
                "${workspaceFolder}/main",
                "${workspaceFolder}/components/board/**",
                "${workspaceFolder}/components/network/**",
                "${workspaceFolder}/components/shell/include",
                "${workspaceFolder}/components/simplegui/**",
                "${env:IDF_PATH}/components/**",
                "${env:IDF_PATH}/components/esp_common/include",
                "${env:IDF_PATH}/components/esp8266/include",
                "${env:IDF_PATH}/components/freertos/port/**",
                "${env:IDF_PATH}/components/freertos/include/private",
                "${env:IDF_PATH}/components/freertos/include",
                "${env:IDF_PATH}/components/esp_ringbuf/include",
                "${env:IDF_PATH}/components/esp_event/include",
                "${env:IDF_PATH}/components/log/include",
                "${env:IDF_PATH}/components/nvs_flash/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__ESP_FILE__=__FILE__"
            ],
            "compilerPath": "${env:ESP_ENV_PATH}/opt/xtensa-lx106-elf/bin/xtensa-lx106-elf-gcc.exe",
            "cStandard": "gnu99",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "windows-gcc-x64"
        }
    ],
    "version": 4
}